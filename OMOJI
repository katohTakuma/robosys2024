import sys
import re

def process_input(input_text):
    cleaned_text = input_text.strip().replace(" ", "")  # 空白を除去
    if not re.match(r'^[a-zA-Z]*$', cleaned_text):  # 英字(A-Z, a-z)以外が含まれている場合
        return "変換できません"
    return input_text.strip().upper()  # すべて大文字に変換して返す

if __name__ == "__main__":
    input_text = sys.stdin.read()
    processed_text = process_input(input_text)
    print(processed_text)

